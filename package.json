{
  "name": "hubfly-cli",
  "module": "dist/index.js",
  "version": "0.1.1",
  "type": "module",
  "bin": {
    "hubfly": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "bun index.ts",
    "typecheck": "tsc",
    "build": "bun build ./index.ts --outfile=dist/index.js --minify --target=bun",
    "push": "bun run build && bun publish"
  },
  "devDependencies": {
    "@inquirer/prompts": "^8.0.2",
    "@types/bun": "latest",
    "commander": "^14.0.2",
    "conf": "^15.0.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
